import java.util.ArrayList;
public class Main8 {
	public static void main(String[] args) {
		ArrayList<Character>arr=new ArrayList<>();
		arr.add('a');
		arr.add('b');
		arr.add('c');
		arr.add('d');
		arr.add('e');
		arr.add('f');
		System.out.println(arr);
		
		for(int i=0;i<arr.size();i++)
		{	arr.remove(i);
			
		}
		System.out.println(arr);

	}

}
